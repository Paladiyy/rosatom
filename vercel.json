{
    "builds": [{
        "src": "rosatom/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "rosatom/wsgi.py"
        }
    ],
    "functions": {
    "api/users/**/*.js": {
      "maxDuration": 30
    }
  }
}